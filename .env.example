# IONOS API Key (format: prefix_public.secret)
IONOS_API_KEY=prefix_public.secret

# Domains to update (comma-separated)
IONOS_DOMAINS=example.com,subdomain.example.com

# Update interval in seconds (default: 300 = 5 minutes)
UPDATE_INTERVAL_SECONDS=300

# Log level: DEBUG, INFO, WARN, ERROR (default: INFO)
LOG_LEVEL=INFO
